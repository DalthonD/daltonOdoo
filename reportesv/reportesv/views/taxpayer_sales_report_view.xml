<odoo>
  <data>

    <record model="ir.ui.view" id="taxpayer_sales_report_sql_tree">
      <field name="name">strategiksv taxpayer sales report sql tree</field>
      <field name="res_model">strategiksv.reportesv.sales_taxpayer.report</field>
      <field name="arch" type="xml">
        <tree string="Ventas-Contribuyentes-list">
          <field name="company_id"/>
          <field name="fecha"/>
          <field name="factura"/>
          <field name="cliente"/>
          <field name="nrc"/>
          <field name="estado"/>
          <field name="gravado"/>
          <field name="exento"/>
          <field name="iva"/>
          <field name="retenido"/>
          <field name="percibido"/>
        </tree>
      </field>
    </record>

  <record model="ir.actions.act_window" id="sv_action_reporteVentasContribuyentes">
		<field name="name">Ventas a Contribuyentes</field>
		<field name="res_model">strategiksv.reportesv.sales_taxpayer.report</field>
		<field name="view_type">tree</field>
		<field name="view_mode">tree,form</field>
		<field name="help" type="html">
			<p class="oe_view_nocontent_create">Ventas a Contribuyentes</p>
		</field>
	</record>


    <menuitem id="main_strategiksv_taxpayersalesreportsv_menu" name="Administracion de Reporte de Ventas a Contribuyentes"/>
    <menuitem id="taxpayer_sales_reportsv_menu" name="Reporte de Compras"
      parent="main_strategiksv_taxpayersalesreportsv_menu"/>
    <menuitem id="reporteVentasCont_menu" name="Ventas_Contribuyentes"
        parent="taxpayer_sales_reportsv_menu"
        action="sv_action_reporteVentasContribuyentes"/>
  </data>
</odoo>
