<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <report
      id="purchase_report_pdf"
      string=" Strateg-k Purchase Report"
      model="report_wizard"
      report_type="qweb-pdf"
      name="purchase_report.strategiksv_purchase_report_pdf"
      file="purchase_report.strategiksv_purchase_report_pdf"
      attachment_use="True"
      attachment="Reporte_De_Compras.pdf"
      />

      <report
        id="taxpayer_report_pdf"
        string=" Strateg-k Taxpayer Report"
        model="report_wizard"
        report_type="qweb-pdf"
        name="taxpayer_report.strategiksv_taxpayer_report_pdf"
        file="taxpayer_report.strategiksv_taxpayer_report_pdf"
        attachment_use="True"
        attachment="Reporte_De_Ventas_Contribuyentes.pdf"
        />

        <report
          id="consumer_report_pdf"
          string=" Strateg-k Consumer Report"
          model="report_wizard"
          report_type="qweb-pdf"
          name="consumer_report.strategiksv_consumer_report_pdf"
          file="consumer_report.strategiksv_consumer_report_pdf"
          attachment_use="True"
          attachment="Reporte_De_Ventas_Consumidores.pdf"
          />

      <record id="purchase_report_action" model="ir.actions.report">
          <field name="name">Reporte de Compras</field>
          <field name="model">report_wizard</field>
          <field name="report_type">qweb-pdf</field>
          <field name="report_name">purchase_report.purchase_report_pdf</field>
          <field name="print_report_name">'Reporte de Compras(PDF)'</field>
      </record>

