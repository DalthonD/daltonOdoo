<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="cryo_sv.pos_config" model="ir.ui.view">
            <field name="name">cryo_sv.pos_config</field>
            <field name="model">pos.config</field>
            <field name="inherit_id" ref="point_of_sale.pos_config_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='module_account']" position="after">
						<field name="header_ticket"/>  
						<field name="footer_ticket"/>  
						<field name="header_recibo"/>  
						<field name="footer_recibo"/>  
                </xpath>
            </field>
        </record>
    
        
  </data>   
</odoo>

